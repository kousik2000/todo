#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules/semver/bin/node_modules:/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules/semver/node_modules:/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/fork-ts-checker-webpack-plugin@6.5.2_eslint@8.34.0+webpack@5.75.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/home/theia/myapp/node_modules/.pnpm/fork-ts-checker-webpack-plugin@6.5.2_eslint@8.34.0+webpack@5.75.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules/semver/bin/node_modules:/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules/semver/node_modules:/home/theia/myapp/node_modules/.pnpm/semver@7.3.8/node_modules:/home/theia/myapp/node_modules/.pnpm/node_modules:/home/theia/myapp/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/myapp/node_modules/.pnpm/fork-ts-checker-webpack-plugin@6.5.2_eslint@8.34.0+webpack@5.75.0/node_modules/fork-ts-checker-webpack-plugin/node_modules:/home/theia/myapp/node_modules/.pnpm/fork-ts-checker-webpack-plugin@6.5.2_eslint@8.34.0+webpack@5.75.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.3.8/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.3.8/node_modules/semver/bin/semver.js" "$@"
fi
